{
  "name": "fp-tetris",
  "version": "0.0.9",
  "description": "library for fp-tetris-game",
  "homepage": "https://github.com/afrontend/fp-tetris",
  "author": {
    "name": "Bob Hwang",
    "email": "frontend.wordpress@gmail.com",
    "url": "https://agvim.wordpress.com"
  },
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "bin": {
    "fp-tetris": "./src/example.js"
  },
  "keywords": [
    "javascript",
    "tetris",
    "game"
  ],
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "prettier": "^1.15.2",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-config-xo": "^0.25.0",
    "jest": "^23.5.0"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "start": "node ./src/example.js"
  },
  "repository": "https://github.com/afrontend/fp-tetris.git",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "clear": "^0.1.0",
    "commander": "^2.19.0",
    "fp-panel": "0.0.3",
    "keypress": "^0.2.1",
    "lodash": "^4.17.11"
  }
}
